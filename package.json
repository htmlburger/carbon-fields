{
  "name": "carbon-fields",
  "version": "1.5.0.0",
  "description": "WordPress developer-friendly custom fields for post types, taxonomy terms, users, comments, widgets, options, navigation menus and more.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "postcss": "node_modules/.bin/postcss"
  },
  "scripts": {
    "test": "jest --config .jest",
    "postcss": "postcss -c postcss.config.js -w",
    "webpack:vendor": "webpack --config webpack.vendor.js",
    "webpack:app": "webpack --config webpack.app.js",
    "webpack:boot": "webpack --config webpack.boot.js",
    "react": "npm run webpack:vendor && concurrently --kill-others --prefix \"[{name}]\" --names \"Webpack:App,Webpack:Boot\" -c \"bgBlue.bold,bgYellow.bold,bgGreen.bold\" \"npm run webpack:app -- -w\" \"npm run webpack:boot -- -w\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htmlburger/carbon-fields.git"
  },
  "keywords": [
    "custom",
    "fields"
  ],
  "author": "htmlburger",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/htmlburger/carbon-fields/issues"
  },
  "homepage": "https://github.com/htmlburger/carbon-fields#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.1.0",
    "cssnano": "^3.8.0",
    "enzyme": "^2.7.0",
    "expose-loader": "^0.7.1",
    "jest": "^18.1.0",
    "jquery": "1.12",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.3",
    "postcss-simple-vars": "^3.0.0",
    "watch": "^1.0.1",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.2",
    "object-path-immutable": "^0.5.1",
    "react": "^15.4.0",
    "react-color": "^2.10.0",
    "react-dom": "^15.4.0",
    "react-onclickoutside": "^5.8.3",
    "react-redux": "5.0.0-beta.3",
    "recompose": "^0.20.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.0.1",
    "redux-saga": "^0.13.0",
    "reselect": "^2.5.4"
  }
}
